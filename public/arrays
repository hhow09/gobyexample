      var codeLines = [];
      codeLines.push('');codeLines.push('package main\u000A');codeLines.push('import \"fmt\"\u000A');codeLines.push('func main() {\u000A');codeLines.push('    var a [5]int\u000A    fmt.Println(\"emp:\", a)\u000A');codeLines.push('    a[4] \u003D 100\u000A    fmt.Println(\"set:\", a)\u000A    fmt.Println(\"get:\", a[4])\u000A');codeLines.push('    fmt.Println(\"len:\", len(a))\u000A');codeLines.push('    b :\u003D [5]int{1, 2, 3, 4, 5}\u000A    fmt.Println(\"dcl:\", b)\u000A');codeLines.push('    var twoD [2][3]int\u000A    for i :\u003D 0; i \u003C 2; i++ {\u000A        for j :\u003D 0; j \u003C 3; j++ {\u000A            twoD[i][j] \u003D i + j\u000A        }\u000A    }\u000A    fmt.Println(\"2d: \", twoD)\u000A');codeLines.push('    var arr [5]int\u000A    arr[4] \u003D 100\u000A    arr2 :\u003D [5]int{0, 0, 0, 0, 100}\u000A    fmt.Println(\"arr \u003D\u003D arr2: \", arr \u003D\u003D arr2)\u000A');codeLines.push('    arr \u003D [5]int{0, 0, 0, 0, 100}\u000A    arr3 :\u003D arr\u000A    arr3[4] \u003D 200\u000A    fmt.Println(\"arr[4] \u003D \", arr[4])\u000A');codeLines.push('    arr \u003D [5]int{1, 2, 3, 4, 100}\u000A    rev(\u0026arr) // should pass array with refrence\u000A    fmt.Println(\"reversed arr \u003D \", arr)\u000A}\u000A');codeLines.push('func rev(arr *[5]int) {\u000A    n :\u003D len(arr)\u000A    for i :\u003D 0; i \u003C n/2; i++ {\u000A        arr[i], arr[n-1-i] \u003D arr[n-1-i], arr[i]\u000A    }\u000A}\u000A');codeLines.push('');
    </script>
